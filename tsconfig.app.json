{
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "include": ["env.d.ts", "resources/ts/**/*", "resources/ts/**/*.vue",],
    "exclude": ["resources/ts/**/__tests__/*"],
    "compilerOptions": {
        "composite": true,
        "noImplicitAny": false,
        "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",

        "baseUrl": ".",
        "paths": {
            "@/*": ["./resources/ts/*"]
        }
    }
}


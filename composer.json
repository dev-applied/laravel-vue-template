{
    "name": "dev-applied/laravel-vue-template",
    "type": "project",
    "description": "Applied Imagination Laravel Vue Template.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://composer.dev-applied3.com"
        },
        {
            "type": "vcs",
            "url": "https://github.com/laravel-shift/laravel-typescript.git"
        }
    ],
    "require": {
        "php": "^8.3",
        "imagine/imagine": "^1.3",
        "laravel/cashier": "^15.7",
        "laravel/framework": "^12",
        "laravel/tinker": "^2.9",
        "league/flysystem-aws-s3-v3": "^3.29",
        "rickycezar/laravel-jwt-impersonate": "^1.3",
        "sentry/sentry-laravel": "^4.9",
        "spatie/laravel-permission": "^6.9.0",
        "spatie/laravel-settings": "^3.3.2",
        "tymon/jwt-auth": "^2.1.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/boost": "^1.0",
        "laravel/pint": "^1.24",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-type-coverage": "^4.0",
        "scrumble-nl/laravel-model-ts-type": "^10.5",
        "spatie/laravel-ray": "^1.40"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php artisan jwt:secret"
        ],
        "format": "./vendor/bin/pint --parallel",
        "ci": [
            "./vendor/bin/pint --test",
            "./vendor/bin/pest --parallel"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
